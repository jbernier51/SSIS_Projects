                      expressionType="Notify"
                      name="TableName"></property>
                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      expressionType="Notify"
                      name="SqlCommand">SELECT 
 
rpupmj, rpsfx,   rpaid2,    rpdgj,       trim(RPDOC) as INVOICE_NO,
   RPPST as PayStatus,             cast(date(char(RPDIVJ + 1900000)) as timestamp) as INVOICE_DATE,
                RPDCT as INVOICE_TYPE,
                cast(date(char(RPDDJ + 1900000)) as timestamp) as INVOICE_DUE_DATE,
                              trim(RPAG) as AMOUNT,
 trim(RPACR)  as AMOUNT_CURRENCY    ,         trim( RPCRCD) as CURRENCY,
                trim(RPPTC) as PAYMENT_TERMS,
                ' '  as PAYMENT_TERMS_DESCRIPTION,
                trim(RPAN8) as CUSTOMER_NO,
                trim(ABALPH) as CUSTOMER_NAME, 
                trim(MCCO) as COMPANY,
CASE WHEN 
 trim((SELECT DISTINCT MCRP12 FROM  JPDTAFR73.F0006 F0006 WHERE MCRP12 &lt;&gt; '' and MCCO = F0311.RPCO and MCRP02 &lt;&gt; 'AFI' and MCSTYL = 'BS')) = 'SE' THEN 'SE' 
	WHEN trim((SELECT DISTINCT MCRP12 FROM JPDTAFR73.F0006 F0006 WHERE MCRP12 &lt;&gt; '' and MCCO = F0311.RPCO and MCRP02 &lt;&gt; 'AFI' and MCSTYL = 'BS')) = 'CS' AND  TRIM(F0311.RPCO) IN 
 ('00350','00550') THEN 'CZ' 
	ELSE trim((SELECT DISTINCT MCRP12 FROM JPDTAFR73.F0006 F0006 WHERE MCRP12 &lt;&gt; '' and MCCO = F0311.RPCO and MCRP02 &lt;&gt; 'AFI' and (MCSTYL = 'BS' 
OR trim(MCDC)='BALANCESHEET')))
	END  as COUNTRY_ID, 
trim(F0006.MCMCU) as LOCATION_CODE,
	          trim(F0006.MCDL01) as LOCATION_NAME,
             trim(F0006.MCRP01) as SiteID,
    
                trim(rpag)  as ORIGINAL_DUE_AMOUNT            
FROM
                JPDTAFR73.F0311  F0311
                left outer join JPDTAFR73.F0101 F0101 on F0311.RPAN8 = F0101.ABAN8
                left outer join JPDTAFR73.F0006 F0006 on F0311.rpmcu = F0006.mcmcu
WHERE
	      F0311.RPAN8 =  F0101.ABAN8
              and          F0311.rpmcu = F0006.mcmcu
              AND ABAT1 &lt;&gt;'O'
--and f0311.rpaap &lt;&gt; 0
--and F0311.RPCO='00760' --Test companys data
and trim(F0311.RPCO) NOT IN ('00232','00585','00550','00205','00206','00207','00208','00210','00211','00212','00216','00217')
and ( trim(RPDCTM)='' or trim(RPDCTM)=' ')
and trim(RPTRTC )&lt;&gt;'G'
and trim(RPGLC)  NOT IN ('SDEP', 'CLMS', 'OTHR','EMPL')
and rpdgj &gt; 110000 and rpupmj &gt;=0</property>
