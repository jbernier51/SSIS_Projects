  HR_ALL.ORGANIZATION_ID || ' - ' || HR_ALL.NAME ORGANIZATION_ID,
  GLCC.SEGMENT1 ENTITY,
 Adjustment_ID	 TRANSACTION_ID, 
  adj.GL_DATE	TRANSACTION_DATE,  
  to_char(adj.GL_DATE,'yyyymmdd') 	TRANSACTION_DT,  

 -- RCTA.TRX_NUMBER	INVOICE_NO, 
  --RCTA.TRX_DATE	INVOICE_DATE, 
 -- to_char(RCTA.TRX_DATE,'yyyymmdd')	INVOICE_DT, 
 
   'Adjustment' as TRXType, 
  ACCTD_AMOUNT  as ACCTD_AMOUNT_APPLIED_TO,
   trx_number as Invoice_no,
  --ARR.CASH_RECEIPT_ID CASH_RECEIPT_ID, 
 -- HCA.ACCOUNT_NUMBER	CUSTOMER_NO, 
  --HP.PARTY_NAME CUSTOMER_NAME,
  --ARCA.STATUS	PAYMENT_STATUS,
 -- ARR.ACCTD_AMOUNT_APPLIED_FROM AMOUNT, 

amount,

--ACCTD_EARNED_DISCOUNT_TAKEN,
--arca.amount as crhamount,
--arca.ACCTD_AMOUNT,
--cast(arca.EXCHANGE_RATE as char(100)) as ARCAExchangeRate,


  SOB.CURRENCY_CODE, 
  --RT.NAME PAYMENT_TERMS,
 -- RT.DESCRIPTION PAYMENT_TERMS_DESCRIPTION,
/*  -- Commented for MicroSoft to test the query
    ($$SOURCE_SYSTEM_ID) SOURCE_SYSTEM_ID,  
    ($$SOURCE_INSTANCE_ID) SOURCE_INSTANCE_ID,
*/
  0 ETL_PROC_WID,
  88 INTEGRATION_ID,
  sysdate W_INSERT_DT, 
  sysdate W_UPDATE_DT,
  'N' DELETE_FLG,
 segment2

FROM 
--apps.AR_RECEIVABLE_APPLICATIONS_ALL ARR,
AR_ADJUSTMENTS_ALL adj,
apps.GL_SETS_OF_BOOKS SOB, 
apps.RA_CUSTOMER_TRX_ALL RCTA,
--apps.HZ_CUST_ACCOUNTS HCA,
--apps.HZ_PARTIES HP,
--apps.RA_TERMS RT,
apps.HR_ALL_ORGANIZATION_UNITS HR_ALL,
apps.HR_LOCATIONS_ALL HR_LOC,
apps.RA_CUST_TRX_TYPES_ALL  RCTT,
apps.GL_CODE_COMBINATIONS GLCC
WHERE  
   adj.customer_trx_id= RCTA.CUSTOMER_TRX_ID(+)
   -- ARR.APPLIED_CUSTOMER_TRX_ID= ADJ.CUSTOMER_TRX_ID(+)
--AND RT.TERM_ID(+)= RCTA.TERM_ID 
--AND ( RCTT.CUST_TRX_TYPE_ID(+) =RCTA.CUST_TRX_TYPE_ID and RCTT.ORG_ID(+)=RCTA.ORG_ID)

and  adj.CODE_COMBINATION_ID=GLCC.CODE_COMBINATION_ID 
AND adj.set_of_books_id= sob.set_of_books_id  
--AND adj. = HCA.CUST_ACCOUNT_ID
--AND HCA.PARTY_ID = HP.PARTY_ID
AND adj.ORG_ID = HR_ALL.ORGANIZATION_ID 
AND HR_ALL.LOCATION_ID = HR_LOC.LOCATION_ID (+)

--and HCA.CUSTOMER_TYPE &lt;&gt; 'I'
and to_char(adj.GL_posted_DATE,'yyyymmdd') &gt;= '20110901'
and (lower(RCTT.DESCRIPTION)  not like '%inter%company%' or lower(RCTT.DESCRIPTION) is null)
and HR_ALL.ORGANIZATION_ID&lt;&gt;'575' --Intercompany
--and rcta.trx_number  = '324628'
--and Adjustment_ID = '3064182'</property>
