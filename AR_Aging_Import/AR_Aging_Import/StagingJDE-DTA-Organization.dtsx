    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{8BFE6CD2-A478-4545-900C-5D51C320F48B}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;select  DISTINCT MCCO, MCRP12 Country ,&#xA;  CASE MCRP01&#xA;     WHEN '   ' THEN '0000'&#xA;     ELSE MCRP01&#xA;END as SiteID ,&#xA;CASE MCRP01&#xA;     WHEN '   ' THEN 'Default' &#xA;     ELSE trim(T01.DRDL01)&#xA;END as Location ,&#xA;  'JDE_LOCATION' SiteType&#xA;&#xA;FROM &quot; + @[User::DTA_Lib]  +   &quot;.F0006 F0006&#xA;INNER JOIN &quot; + @[User::DTA_Lib]  +   &quot;.F0902 F0902&#xA;  ON F0902.GBMCU = F0006.MCMCU&#xA;  AND F0902.GBCO = F0006.MCCO&#xA;LEFT OUTER JOIN &quot; + @[User::COM_Lib]  +   &quot;.F0005 T01&#xA;  ON TRIM(T01.DRKY)     = TRIM( F0006.MCRP01 )&#xA;  AND ( TRIM( T01.DRSY) = '00'&#xA;  AND TRIM(T01.DRRT)    = '01')&#xA;WHERE&#xA;MCSTYL not in ('MD','MC','MO','CL','AF','')&#xA;&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="JDESourceQuery">
      <DTS:VariableValue
        DTS:DataType="8">select  DISTINCT MCCO, MCRP12 Country ,
  CASE MCRP01
     WHEN '   ' THEN '0000'
     ELSE MCRP01
END as SiteID ,
CASE MCRP01
     WHEN '   ' THEN 'Default' 
     ELSE trim(T01.DRDL01)
END as Location ,
  'JDE_LOCATION' SiteType

FROM JPDTALU73.F0006 F0006
INNER JOIN JPDTALU73.F0902 F0902
  ON F0902.GBMCU = F0006.MCMCU
  AND F0902.GBCO = F0006.MCCO
LEFT OUTER JOIN JPCOMUK73.F0005 T01
  ON TRIM(T01.DRKY)     = TRIM( F0006.MCRP01 )
  AND ( TRIM( T01.DRSY) = '00'
  AND TRIM(T01.DRRT)    = '01')
WHERE
MCSTYL not in ('MD','MC','MO','CL','AF','')
</DTS:VariableValue>
    </DTS:Variable>
