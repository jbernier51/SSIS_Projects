                        <property
                          dataType="System.String"
                          description="The SQL command to be executed."
                          expressionType="Notify"
                          name="SqlCommand">select  DISTINCT MCCO, MCRP12 Country ,
  CASE MCRP01
     WHEN '   ' THEN '0000'
     ELSE MCRP01
END as SiteID ,
CASE MCRP01
     WHEN '   ' THEN 'Default' 
     ELSE trim(T01.DRDL01)
END as Location ,
  'JDE_LOCATION' SiteType

FROM JPDTALU73.F0006 F0006
INNER JOIN JPDTALU73.F0902 F0902
  ON F0902.GBMCU = F0006.MCMCU
  AND F0902.GBCO = F0006.MCCO
LEFT OUTER JOIN JPCOMUK73.F0005 T01
  ON TRIM(T01.DRKY)     = TRIM( F0006.MCRP01 )
  AND ( TRIM( T01.DRSY) = '00'
  AND TRIM(T01.DRRT)    = '01')
WHERE
MCSTYL not in ('MD','MC','MO','CL','AF','')
